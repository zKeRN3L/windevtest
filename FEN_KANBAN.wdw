     p_codes :
      -
        internal_properties : CAAAAAgAAADlz+AqkziugtVWk3ywm+7/tec2AH5r79f11C9SXEHpKfdCMAT17n0ddrUzOsBuEwTl6Ni45h5fs/c+vR6XKeW9NIihzKgBkfNZW49FdbOPI6L44eChRFtKCZn6td4KumkIcETS/QBb0YYo2YI=
        code : |1+
         
         sFichier est une chaine
         
         // Ouvre le sélecteur de fichiers
         sFichier = fSélecteur( "", "", "Enregistrer sous...", "Tous les fichiers (*.*)" + TAB + "*.*" + RC + "Kanban (*.wdkanban)" + TAB + "*.WDKANBAN", "WDKANBAN", fselOuvre + fselExiste )
         si sFichier = "" alors
         	DonneFocusEtRetourUtilisateur()
         fin		
         
         // Démarre sur un kanban vierge
         BTN_NOUVEAU_KANBAN.ExécuteTraitement( trtclic )
         
         // Mémorise
         FichierKanban = sFichier
         
         stDescriptionKanban est une DescriptionKanban
         
         // Charge et désérialise
         bufKanban est un buffer = fChargeBuffer( FichierKanban )
         deserialise( stDescriptionKanban, bufKanban, psdXML )
         
         // Ajoute les cartes dans le kanban
         stDescriptionKanban.ListeEnAttente.Applique	( Carte => LISTE_ENATTENTE.AjouteCarte	( Carte ) )
         stDescriptionKanban.ListeEnCours.Applique	( Carte => LISTE_ENCOURS.AjouteCarte	( Carte ) )
         stDescriptionKanban.ListeTerminée.Applique	( Carte => LISTE_TERMINE.AjouteCarte	( Carte ) )
        type : 18
      -
        type : 161
        enabled : false
