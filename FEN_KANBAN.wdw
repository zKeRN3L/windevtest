  -
    name : BTN_NOUVEAU_KANBAN
    identifier : 0x2374a14a0779509b
    internal_properties : 
    properties :
     x : 12
     y : 6
     width : 57
     height : 68
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1-
         
         // Si on n'a pas enregistré le kanban en cours
         si FichierKanban = "" ALORS
         	
         	// Si le kanban en cours a été modifié
         	si KANBAN_TACHES.Modifié = vrai ALORS
         		
         		// Demande confirmation pour l'enregistrement
         		selon confirmer( Annuler, "Les modifications n'ont pas été enregistrées. Souhaitez-vous enregistrer le kanban maintenant ?" )
         			
         			// Enregistre
         			CAS oui
         				
         				// Si l'enregistrement échoue, on annule
         				si BTN_ENREGISTRER.ExécuteTraitement( trtclic ) = faux ALORS
         					DonneFocusEtRetourUtilisateur()
         				FIN
         
         			// Annule
         			cas annuler
         				DonneFocusEtRetourUtilisateur()
         
         			// Poursuit
         			cas non
         		FIN
         	FIN
         FIN
         
         // Supprime toutes les cartes
         KANBAN_TACHES.SupprimeToutCarte()
         
         // RAZ
         KANBAN_TACHES.Modifié = faux
         FichierKanban = ""
        type : 18
      -
        type : 161
        enabled : false
  -
    name : BTN_ENREGISTRER
    identifier : 0x2374a18d078e53c3
    internal_properties : 
    properties :
     x : 141
     y : 6
     width : 181
     height : 68
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        internal_properties : CAAAAAgAAABtR3iyB2mPguU2YzyIMzbPTYfWoB57j18dvMcK7HEpcbd66PxtlsVtNnXTaqgWeyTt2Pi4xj7zJ3OiKZpL5aFBSPyl0MydxYdVH8sBKe/bJ0bsNXRV0O+ORS1mERJ2xm2MlOiW0XXHWR6wUQo=
        code : |1-
         
         // Si le kanban est vide
         si lISTE_ENATTENTE.Carte.Vide _et_ LISTE_ENCOURS.Carte.Vide _et_ LISTE_TERMINE.Carte.Vide ALORS
         	
         	// Si on est sur un nouveau kanban
         	si FichierKanban = "" alors
         		
         		// Pas de sauvegarde	
         		RENVOYER Vrai
         	fin	
         FIN
         
         // Si on n'a pas de fichier
         si FichierKanban = "" ALORS
         	
         	sFichier est une chaine
         	
         	// Ouvre le sélecteur de fichiers
         	sFichier = fSélecteur( "", "", "Enregistrer sous...", "Tous les fichiers (*.*)" + TAB + "*.*" + RC + "Kanban (*.wdkanban)" + TAB + "*.WDKANBAN", "WDKANBAN", fselCrée + fselExiste )
         	si sFichier = "" alors		
         		RENVOYER faux
         	fin		
         
         	// Mémorise le nom du fichier	
         	FichierKanban = sFichier
         FIN
         
         stDescriptionKanban est une DescriptionKanban
         
         stDescriptionKanban.ListeEnAttente 	<= LISTE_ENATTENTE.Carte
         stDescriptionKanban.ListeEnCours 	<= LISTE_ENCOURS.Carte
         stDescriptionKanban.ListeTerminée	<= LISTE_TERMINE.Carte
         
         // Sérialise et sauve sur le disque
         bufKanban est un buffer
         serialise( stDescriptionKanban, bufKanban, psdXML )
         fSauveBuffer( FichierKanban, bufKanban )
         
         renvoyer vrai
        type : 18
      -
        type : 161
        enabled : false
